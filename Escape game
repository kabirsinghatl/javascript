        vertex(x + 32, y - 30);
        vertex(x + 42, y + 10);
        vertex(x + 10, y + 30);
        vertex(x - 30, y + 20);
        vertex(x - 40, y - 20);
        endShape();
    }, "story", "Story", 150);
    
    var skinB = new Button(300, 300, 440, 400, function(x, y) {
        rect(x - 25, y - 25, 50, 50);
        fill(90, 90, 100, this.fade);
        rect(x - 15, y - 10, 10, 10);
        rect(x + 5, y - 10, 10, 10);
    }, "skins", "Skins", 150);
    
    var mapB = new Button(300, 300, 300, 500, function(x, y) {
        ellipse(x, y, 50, 50);
