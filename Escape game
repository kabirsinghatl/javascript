        beginShape();
        vertex(-97, 120);
        vertex(0, 120);
        vertex(12, -210);
        bezierVertex(-50, -70, -80, 30, -97, 120);
        endShape();
        popMatrix();
    };
    
    var spaceship3 = function(x, y, r, size, fire, s) {
        noStroke();
        pushMatrix();
        translate(x, y);
        scale(size / 400, size / 400 + s);
