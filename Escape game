        image(crateImg, 490, 410, 130, 130);
        image(crateImg, 410, 260, 130, 130);
        pushMatrix();
        translate(30, 520);
        scale(0.3, 0.3);
        characterDesigns[curCharacter]({moveX : 0, moveY : -40});
