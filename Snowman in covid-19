mousePressed = function () {
    //snow
    fill(mouseX, mouseY, mouseY);
    ellipse(mouseX, mouseY,15,15);
};
fill(255, 255, 255);
ellipse(200, 300, 150, 150);
ellipse(200, 200, 100, 100);
ellipse(200, 120, 75, 75);
fill(0, 0, 0);
ellipse(183,110,10,10);
