    rect(314, 215, 6, 2);
    rect(326, 235, 6, 2);
    rect(325, 259, 6, 2);
    rect(334, 280, 6, 2);
    rect(318, 248, 6, 2);
    rect(314, 277, 6, 2);
    rect(330, 221, 6, 2);

    rect(365, 193, 4, 2);
    rect(379, 175, 2, 4);
    rect(384, 170, 2, 4);
    rect(374, 183, 2, 4);
    rect(359, 166, 2, 3);
    rect(364, 159, 5, 2);
    rect(371, 159, 5, 2);
    rect(379, 159, 4, 2);
    rect(371, 152, 5, 2);

    fill(0, 0, 0, 60);
    rect(364, 206, 4, 2);
    rect(380, 223, 4, 2);
