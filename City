    rect(326, 146, 22, 20);
    rect(332, 146, 16, 20);
    rect(337, 146, 10, 20);
    rect(342, 146, 2, 20);
    fill(110, 48, 48);
    triangle(325, 147, 349, 147, 338, 138);
    arc(338, 146, 22, 5, 0, 180);
    popMatrix();
};
var skyline = function(x, y, sz, sy) {
    pushMatrix();
    translate(x, y);
    scale(sz, sy);
    rect(12, 110, 25, 100);
    rect(38, 130, 15, 100);
    rect(52, 98, 25, 100);
    rect(136, 81, 25, 100);
    rect(150, 126, 25, 100);
    rect(182, 98, 25, 150);
