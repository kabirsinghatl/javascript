    leftWindow(x - 30, y + 110, sz);
    leftWindow(x - 30, y + 150, sz);
    leftWindow(x - 30, y + 190, sz);
    leftWindow(x - 30, y + 230, sz);
    BluewaterTower(x - 318, y + 131, sz);
    popMatrix();
};
