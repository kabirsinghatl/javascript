var waterTower = function(x, y, sz) {
    pushMatrix();
    translate(x, y);
    scale(sz);

    fill(148, 146, 146);
    rect(326, 155, 2, 3);
    rect(332, 155, 2, 3);
    rect(329, 156, 2, 3);
    fill(166, 80, 63);
    triangle(326, 146, 334, 146, 330, 141);
    fill(194, 142, 93);
    rect(326, 146, 8, 10);
    fill(255, 255, 255, 30);
    rect(328, 146, 6, 10);
    rect(329, 146, 5, 10);
    rect(331, 146, 3, 10);
    rect(332, 146, 2, 10);
    popMatrix();
};
var BluewaterTower = function(x, y, sz) {
    pushMatrix();
    translate(x, y);
    scale(sz);

    fill(37, 44, 54);
    rect(326, 165, 2, 5);
    rect(345, 165, 2, 5);
    rect(335, 166, 2, 5);
