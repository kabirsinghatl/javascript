var cloud = 0;
var cloud2 = 0;
var cloud3 = 0;
noStroke();
var Clouds = function(x, y, sz) {
    pushMatrix();
    translate(x, y);
    scale(sz);
    fill(255, 255, 255, 190);
    rect(cloud + 60,50,130,10,15);
    ellipse(cloud + 68,50,20,20);
    ellipse(cloud + 181,50,20,20);
    ellipse(cloud + 89,45,25,25);
    ellipse(cloud + 99,50,15,15);
    ellipse(cloud + 119,41,35,35);
    ellipse(cloud + 138,45,25,25);
    ellipse(cloud + 161,44,28,27);
    
    rect(cloud3 + 60,200,130,10,15);
    ellipse(cloud3 + 68,200,20,20);
    ellipse(cloud3 + 181,200,20,20);
    ellipse(cloud3 + 89,195,25,25);
    ellipse(cloud3 + 99,200,15,15);
    ellipse(cloud3 + 119,191,35,35);
    ellipse(cloud3 + 138,195,25,25);
    ellipse(cloud3 + 161,194,28,27);
    
    rect(cloud2 + 60,150,130,10,15);
    ellipse(cloud2 + 68,150,20,20);
    ellipse(cloud2 + 181,150,20,20);
    ellipse(cloud2 + 89,145,25,25);
    ellipse(cloud2 + 99,150,15,15);
    ellipse(cloud2 + 119,141,35,35);
    ellipse(cloud2 + 138,145,25,25);
    ellipse(cloud2 + 161,144,28,27);
    
    rect(cloud3 + -130,75,130,10,15);
    ellipse(cloud3 + -122,75,20,20);
    ellipse(cloud3 + -9,75,20,20);
    ellipse(cloud3 + -101,70,25,25);
    ellipse(cloud3 + -91,75,15,15);
    ellipse(cloud3 + -71,66,35,35);
    ellipse(cloud3 + -52,70,25,25);
    ellipse(cloud3 + -29,69,28,27);

    cloud += 1;
    if (cloud > 750) {
        cloud = -200;
    }
    cloud2 += 1.5;
    if (cloud2 > 750) {
        cloud2 = -150;
    }
    cloud3 += 0.7;
    if (cloud3 > 800) {
        cloud3 = -150;
    }
    popMatrix();
};
var skyscraper1 = function(x, y, sz) {

    pushMatrix();
    translate(x, y);
    scale(sz);

    fill(158, 153, 199);
    rect(200, 200, 63, 94);
    fill(95, 100, 128);
    quad(200, 200, 200, 303, 195, 303, 195, 212);
    fill(72, 66, 94);
    quad(263, 197, 263, 300, 285, 303, 285, 190);
    fill(214, 182, 154);
    quad(294, 168, 330, 184, 330, 303, 285, 271);

    for (var i = 254; i < 342; i += 5) {
        fill(210, 215, 245);
        rect(203, i - 51, 57, 2);
    }
    for (var i = 254; i < 306; i += 6.5) {
        fill(176, 173, 201);
        rect(i - 47, 202, 2, 91);
    }

    fill(61, 63, 112, 140);
    quad(263, 200, 263, 300, 211, 303, 252, 200);

    fill(69, 73, 99);

    beginShape();
    vertex(283, 174);
    vertex(294, 168);
    vertex(297, 269);
    vertex(295, 306);
    vertex(299, 209);
    vertex(307, 206);
    vertex(301, 311);
    vertex(276, 305);
    vertex(285, 227);
    endShape();

    fill(94, 88, 110);

    beginShape();
    vertex(334, 174);
    vertex(351, 166);
    vertex(351, 160);
    vertex(356, 160);
    vertex(369, 147);
    vertex(386, 147);
    vertex(386, 197);
    vertex(389, 197);
    vertex(389, 299);
    vertex(326, 299);
    vertex(330, 203);
    vertex(334, 203);
    endShape();

    for (var i = 254; i < 279; i += 3) {
        fill(54, 57, 71);
        rect(i + 81, 175, 1, 124);
    }

    fill(214, 194, 184);
    quad(341, 206, 351, 300, 300, 303, 307, 206);

    fill(166, 170, 189);
    rect(357, 164, 33, 35);
    rect(362, 156, 21, 35);
    rect(368, 150, 18, 35);
