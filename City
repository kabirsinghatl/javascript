    rect(248, 136, 14, 100);
    rect(261, 124, 14, 100);
    rect(273, 102, 35, 100);
    rect(300, 119, 25, 100);
    rect(323, 102, 35, 100);
    rect(353, 89, 20, 100);
    rect(465, 67, 20, 100);
    popMatrix();
};
var allskyscrapers = function(x, y, sz, sz2, sy) {
    pushMatrix();
    scale(sz2, sy);
    skyscrapers1(x + 65, y - 20, sz);
    skyscrapers11(x, y, sz);
    skyscrapers10(x, y, sz);
    skyscrapers4(x - 18, y - 0, sz);
    skyscrapers9(x, y - 10, sz);
    skyscrapers7(x, y, sz + 0.1);
    skyscrapers8(x, y, sz + 0.1);
    skyscrapers3(x + 109, y + 30, sz - 0.3);
    skyscrapers2(x + 70, y + -20, sz - 0.04);
    waterTower(x - 116, y + 73, sz, sy);
    skyscrapers5(x, y - 10, sz);
    skyscrapers14(x, y - 20, sz);
    skyscrapers15(x, y - 20, sz);
    skyscrapers6(x, y, sz);
    skyscrapers12(x, y, sz);
    skyscrapers17(x - 10, y, sz);
    skyscrapers16(x + 8, y, sz);
    skyscrapers13(x, y, sz);
    leftWindow(x, y, sz);
    leftWindow(x - 10, y + 10, sz);
    leftWindow(x, y + 40, sz);
    leftWindow(x - 10, y + 50, sz);
    leftWindow(x, y + 80, sz);
    leftWindow(x - 10, y + 90, sz);
    leftWindow(x, y + 120, sz);
    leftWindow(x - 10, y + 130, sz);
    leftWindow(x, y + 160, sz);
    leftWindow(x - 10, y + 170, sz);
    leftWindow(x, y + 200, sz);
    leftWindow(x - 10, y + 210, sz);
    leftWindow(x, y + 240, sz);
    leftWindow(x - 10, y + 250, sz);
    leftWindow(x - 30, y + 30, sz);
